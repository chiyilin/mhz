.member {
  width: 750rpx;
  padding-top: 36rpx;
  background-size: 100%;
  background-color: #FDFFE9;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-bottom: 40rpx;
}
.zhiwu{
  color: #74412A;
  font-size: 30rpx;
  line-height: 80rpx;
}
.money{
  margin: 0 30rpx;
  font-size: 28rpx;
  padding-top: 10rpx;
  border-bottom: 1px solid #E6E6E6;
}
.money text{
  padding-left:48rpx;
  line-height: 66rpx;
}
.grzl{
  width: 146rpx;
}
.touxiang {
  width: 143rpx;
  display: block;
  border-radius: 50%;
  margin: 0 auto;
}
.message {
  position: absolute;
  top: 30rpx;
  right: 34rpx;
}
.xiaoxi {
  width: 45rpx;
}
.message text {
  font-size: 20rpx;
  background-color: #ca1b24;
  color: #fff;
  border-radius: 50%;
  display: block;
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  position: absolute;
  top: 25rpx;
  left: 21rpx;
}

.zc {
  padding: 15rpx 61rpx;
  border-bottom: 1px solid #e6e6e6;
  background-color: #fff
}

.zcimg {
  width: 23rpx;
  vertical-align: bottom;
  display: inline-block;
  margin-right: 10rpx;
}

.zctext {
  font-size: 28rpx;
  vertical-align: bottom;
  display: inline-block;
}

.hy {
  display: flex;
  border-bottom: 9rpx solid #f4f4f4;
  padding-bottom: 27rpx;
  background-color: #fff;
}

.hy view {
  display: flex;
  flex-direction: column;
  width: 50%;
  align-items: center;
}

.hy image {
  width: 59rpx;
  display: block;
  padding: 22rpx 0;
  margin-left: auto;
  margin-right: auto;
}

.hy text {
  display: block;
  font-size: 28rpx;
}

.list {
  height: 93rpx;
  padding: 0 51rpx;
  position: relative;
  background-color: #fff;
}
.list::after{
position: absolute;
bottom: 0;
left: 0;
width: 100%;
height: 1px;
background-color: #e6e6e6;
content: ' ';
display: block;
transform: scaley(0.5);
}

.left image {
  width: 43rpx;
  vertical-align: middle;
  margin-right: 27rpx;
  margin-top: 24rpx;
}

.left text {
  font-size: 28rpx;
  vertical-align: middle;
  margin-top: 28rpx;
  display: inline-block;
}

.right image {
  width: 18rpx;
  margin-top: 25rpx;
}

.left .new {
  margin-left: 20rpx;
  margin-top: 28rpx;
  display: inline-block
}

.jishu {
  font-size: 28rpx;
  background-color: #f4f4f4;
  text-align: center;
}

.jishu text {
  line-height: 209rpx;
  color: #b7b7b7;
}
